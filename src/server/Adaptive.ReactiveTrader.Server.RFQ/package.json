{
  "name": "adaptive.reactivetrader.server.rfq",
  "version": "0.0.1",
  "description": "RFQ Service for Reactive Trader",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'node -r ts-node/register --inspect' src/index.ts",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "precommit": "lint-staged"
  },
  "keywords": [
    "RFQ"
  ],
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "author": "Bhavesh Desai",
  "license": "MIT",
  "devDependencies": {
    "@types/autobahn": "^0.9.42",
    "@types/jest": "^24.0.4",
    "@types/node": "^11.9.3",
    "autobahn": "^18.10.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "nodemon": "^1.18.10",
    "prettier": "1.16.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3",
    "ws": "^3.3.3"
  },
  "dependencies": {
    "@types/debug": "^4.1.2",
    "@types/dotenv": "^6.1.0",
    "@types/uuid": "^3.4.4",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "node-eventstore-client": "^0.1.13",
    "rxjs": "^6.4.0",
    "uuid": "^3.3.2"
  }
}
