#! /bin/bash

build=""
if [[ $1 != "" ]];then
  build=".$1"
fi

echo "Web"

. ../../../config

docker rm web 2&> garbage

command="cp /localhost/nginx.conf /etc/nginx/nginx.conf && nginx -g 'daemon off;'"
docker run -d --net=host \
     --name web          \
     $webContainer$build \
     bash -c "$command"  \
     > container_id 

rm garbage

echo " "
